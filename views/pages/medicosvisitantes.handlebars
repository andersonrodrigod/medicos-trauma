        <div class="tabelas-medicas-semana">
            <div class="dia-semana">
                <h2>Domingo</h2>
                <ul>
                {{#each medicosTurn.morning}}
                    {{#if (eq this.dayOfWeek "sunday")}}
                    <li>{{this.name}} ({{#if (eq doctor.frequency "weekly")}}S{{else}}Q{{/if}})</li>
